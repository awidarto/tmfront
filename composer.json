{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.1.*",
        "jenssegers/mongodb": "*",
        "nitmedia/wkhtml2pdf": "dev-master",
        "noherczeg/breadcrumb": "dev-master",
        "anahkiasen/former": "dev-develop",
        "intervention/image": "2.*",
        "intervention/imagecache": "dev-master",
        "intervention/helper": "dev-master",
        "intervention/validation": "dev-master",
        "alexwhitman/view-themes": "dev-master",
        "mj/breadcrumb": "dev-master",
        "knplabs/knp-snappy": "*",
        "barryvdh/laravel-snappy": "dev-master",
        "pingpong/menus": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
            "app/helpers",
            "app/library"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
